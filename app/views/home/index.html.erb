<div class="center">
  <% if current_user %>
    <%= button_to("Log Out", logout_path, method: :get) %>
  <% else %>
    <%= button_to("Create New User", register_path, method: :get) %>
    <%= button_to("Log In", login_path, method: :get) %>
  <% end %>
</div>
<% if current_user %>
  <h3> Emails of Existing Users: </h3>
  <% @users.each do |user| %>
    <p><%= user.email %></p>
  <% end %>
<% end %>