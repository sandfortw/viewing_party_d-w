<div class="center">
  <%= button_to("Create New User", register_path, method: :get) %>
</div>

<% if session[:user_id] %>
<h2> Users: </h2>
<ul>
<% @users.each do |user| %>
  <li><%= user.email%> </li>
<% end %>
</ul>
  <%= link_to 'Log Out', logout_path%>
<%else %>
  <%= link_to 'Log In', login_path%>
  <%= link_to 'Checkout some movies, visitor!', discover_path %>
<% end %>
  <%= link_to 'My dashboard', dashboard_path %>