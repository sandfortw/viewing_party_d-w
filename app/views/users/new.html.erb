<h1> Viewing Party </h1>


<h3> Register a New User </h3>

<%= form_with url: users_path, method: :post, local: true do |form| %>
  <%= form.label :name, "Name:" %>
  <%= form.text_field :name %>
  <%= form.label :email, "Email:" %>
  <%= form.text_field :email %>
  <%= form.submit "Register" %>
<% end%>